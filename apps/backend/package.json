{
  "name": "jobrun-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build:prisma": "npx prisma generate",
    "build:tsc": "tsc",
    "build": "npm run build:prisma && npm run build:tsc",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:fix": "node scripts/migrate-with-retry.js",
    "start": "node server.js"
  },

  "dependencies": {
    "@prisma/client": "5.22.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "bcryptjs": "^2.4.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^4.77.3",
    "twilio": "^5.10.6",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "ts-node-dev": "^2.0.0",
    "prisma": "5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1"
  }
}
